{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "max-len": "off"
    }
}