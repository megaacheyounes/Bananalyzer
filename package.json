{
  "name": "bananalyzer",
  "version": "1.0.0",
  "description": "A tool to downloads APKs from Google Playstore and lists the GMS and HMS kits that each APK includes and other metadata",
  "author": "Younes Megaache",
  "homepage": "https://github.com/megaacheyounes/bananalyzer",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "displayName": "Banana Analyzer",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "cross-env NODE_ENV=debug ts-node index.ts --keep --reuse",
    "start:debug": "cross-env NODE_ENV=debug ts-node index.ts --keep --reuse --debug",
    "start:package": "cross-env NODE_ENV=debug ts-node index.ts package --keep --reuse --name",
    "debug": "cross-env NODE_ENV=debug npm run start -- --enable-logs",
    "devider1sdsdaas": " __________________ build (pkg) __________________ ",
    "build": "npm run copy-files && npm run clean &&  npm run build:js && npm run build:exe",
    "build2": "npm run clean &&  npm run build:esm && npm run build:exe",
    "copy-files": "copyfiles \"instructions.txt\" \"example_apps.txt\" \"AppCheck.jar\" build",
    "clean": "rimraf dist esm build",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "build:js": "ncc build index.ts -o dist",
    "build:exe": "pkg -t node16-win-x64 --out-path build -C GZip --public  esm/index.js ",
    "distro": "npm run clean && npm run build:esm && yarn distro:ncc && yarn distro:pkg",
    "distro:ncc": "ncc build dist/build/main.js -o dist/ncc",
    "distro:pkg": "pkg -t node10-macos-x64,node10-win-x64 dist/ncc/index.js -o dist/bin/ncc-pkg-demo",
    "devider12341324": " __________________ test __________________ ",
    "test": "jest",
    "test:analyzer": "jest tests/analyzer.test.ts",
    "test:downloader": "jest tests/downloader.test.ts",
    "test:excelHelper": "jest tests/excelHelper.test.ts"
  },
  "bin": "index.ts",
  "keywords": [
    "hms",
    "gms",
    "analyze",
    "playstore",
    "download",
    "apk"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/node": "^17.0.23",
    "cli-meow-help": "^2.0.2",
    "debug": "^4.3.4",
    "decompress-zip": "^0.3.3",
    "download-chromium": "^2.2.1",
    "fetch": "^1.1.0",
    "fs": "0.0.1-security",
    "java-caller": "^2.4.0",
    "meow": "^9.0.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "puppeteer": "^13.5.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "request": "^2.88.2",
    "xlsx": "^0.18.4",
    "yauzl": "^2.7.0"
  },
  "pkg": {
    "scripts": [
      "node_modules/puppeteer/lib/*.js",
      "node_modules/puppeteer-extra-plugin-stealth/**/*.js"
    ],
    "options": [
      "experimental-modules",
      "experimental-json-modules"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/meow": "^6.0.0",
    "@types/mkdirp": "^1.0.2",
    "@types/ncp": "^2.0.5",
    "@types/puppeteer": "^5.4.5",
    "@types/rimraf": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "ncc": "^0.3.6",
    "pkg": "^5.5.2",
    "rimraf": "^2.7.1",
    "ts-jest": "^27.1.4",
    "ts-migrate": "^0.1.28",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "clone-deep": "4.0.1"
  }
}