{
  "name": "bananalyzer",
  "version": "1.0.0",
  "description": "A tool to downloads APKs from Google Playstore and lists the GMS and HMS kits that each APK includes and other metadata",
  "author": "Younes Megaache",
  "homepage": "https://github.com/megaacheyounes/bananalyzer",
  "main": "./dist/index.js",
  "scripts": {
    "start": "ts-node  index.ts --keep-apks --use-existing",
    "debug": "npm run start -- --enable-logs",
    "dev": "ts-node  dev.js ",
    "build": "npm run copy-files && npm run build-impl",
    "build-impl": "pkg -t node16-win-x64 --out-path build -C GZip --public index.ts ",
    "copy-files": "copyfiles \"instructions.txt\" \"example_apps.txt\" \"AppCheck.jar\" build"
  },
  "bin": "index.ts",
  "keywords": [
    "hms",
    "gms",
    "analyze",
    "playstore",
    "download",
    "apk"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/node": "^17.0.23",
    "debug": "^4.3.4",
    "decompress-zip": "^0.3.3",
    "download-chromium": "^2.2.1",
    "fetch": "^1.1.0",
    "fs": "0.0.1-security",
    "java-caller": "^2.4.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "puppeteer": "^13.5.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "request": "^2.88.2",
    "xlsx": "^0.18.4",
    "yauzl": "^2.7.0"
  },
  "pkg": {
    "scripts": [
      "node_modules/puppeteer/lib/*.js",
      "node_modules/puppeteer-extra-plugin-stealth/**/*.js"
    ],
    "options": [
      "experimental-modules",
      "experimental-json-modules"
    ]
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/ncp": "^2.0.5",
    "@types/puppeteer": "^5.4.5",
    "@types/rimraf": "^3.0.2",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pkg": "^5.5.2",
    "rimraf": "^2.7.1",
    "ts-migrate": "^0.1.28",
    "ts-node": "^10.7.0"
  }
}