{
  "name": "bananalyzer",
  "version": "1.0.0",
  "description": "A tool to downloads APKs from Google Playstore and lists the GMS and HMS kits that each APK includes and other metadata",
  "author": "Younes Megaache",
  "homepage": "https://github.com/megaacheyounes/bananalyzer",
  "main": "bananalyzer.js",
  "scripts": {
    "start": "node bananalyzer.js",
    "start-debug": "node bananalyzer.js --logs",
    "build": "npm run copy-libs && npm run build-impl",
    "build-impl": "pkg bananalyzer.js -t node14-win-x64 --out-path build -C GZip --public",
    "copy-libs": "copyfiles \"AppCheck.jar\" build"
  },
  "keywords": [
    "hms",
    "gms",
    "analyze",
    "playstore",
    "download",
    "apk"
  ],
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.7",
    "copyfiles": "^2.4.1",
    "debug": "^4.3.4",
    "decompress-zip": "^0.3.3",
    "download-chromium": "^2.2.1",
    "fs": "0.0.1-security",
    "java-caller": "^2.4.0",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "puppeteer": "^13.5.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "request": "^2.88.2",
    "xlsx": "^0.18.4",
    "yauzl": "^2.7.0"
  },
  "bin": "bananalyzer.js",
  "pkg": {
    "scripts": [
      "node_modules/puppeteer/lib/*.js",
      "node_modules/puppeteer-extra-plugin-stealth/**/*.js"
    ]
  }
}
